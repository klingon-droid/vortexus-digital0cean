# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
ASSISTANT_ID=your_openai_assistant_id_here

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
SOLANA_URL=your_solana_rpc_url_here
ENCRYPTION_KEY=your_32_byte_encryption_key_here
ENCRYPTION_ALGORITHM=aes-256-cbc

# API Configuration
# For local development with ngrok:
# AI_AGENT_API_URL=https://your-ngrok-url.ngrok-free.app/prompt
# For production:
AI_AGENT_API_URL=http://localhost:3000/prompt

# Telegram Bot Webhook URL (only needed if using webhook mode instead of polling)
# Leave blank to use polling mode
# Production example:
# TELEGRAM_WEBHOOK_URL=https://your-domain.com/bot<TELEGRAM_BOT_TOKEN>
TELEGRAM_WEBHOOK_URL=

# Server Configuration
API_PORT=3000
TELEGRAM_PORT=4001

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/arcturus

# CORS Configuration
FRONTEND_URL=http://localhost:3000 